{
  "name": "major-employers",
  "datapackage_version": "1.0-beta.18",
  "title": "Major Employers",
  "description": "Major Employers reports the top five employers by town. Rank is based on the number of employees.",
  "version": "1.0",
  "licenses": [{
    "url": "http://opendatacommons.org/licenses/pddl/",
    "name": "Open Data Commons Public Domain",
    "version": "1.0",
    "id": "odc-pddl"
  }],
  "sources": [{
    "name": "municipalities",
    "web": ""
  }],
  "author":[{
    "name": "Jenna Daly",
    "email": "jdaly@ctdata.org",
    "web": "http://ctdata.org"
  }],
  "contributor": [],
  "resources": [{
    "path": "data/major_employers_2014.csv",
    "format": "csv",
    "schema": {
      "fields": [{
          "name": "Town",
          "type": "string",
          "dimension": false
        }, {
          "name": "FIPS",
          "type": "string",
          "dimension": false
        }, {
          "name": "Year",
          "type": "string",
          "dimension": false
        }, {
          "name": "Year Submitted",
          "type": "string",
          "dimension": false
        }, {
          "name": "Town Profile Year",
          "type": "string",
          "dimension": false
        }, {          
          "name": "Rank",
          "type": "string",
          "dimension": true, 
          "constraints": {
            "enum": ["1", "2", "3", "4", "5"]
          }           
        }, {           
          "name": "Measure Type",
          "type": "string",
          "dimension": true, 
          "constraints": {
            "enum": ["Name"]
          }           
        }, {
          "name": "Variable",
          "type": "string",
          "dimension": true, 
          "constraints": {
            "enum": ["Employer"]
          }           
        }, {
          "name": "Value",
          "type": "string",
          "dimension": false
        }]
      }
    }
  ],
  "last_modified": "05/30/2017",
  "ckan_extras": {
    "full_description": {
      "ckan_name": "Full Description",
      "value": "Major Employers reports the top five employers by town. Rank is based on the number of employees.'Year Submitted' represents when the grand list data was collected for a given year. 'Town Profile Year' represents for which year these data were used in the Town Profiles project.",
      "type": "string"
    },
    "suppression": {
